# PATHS
#
# Basic paths that are common to all my machines

alias home="cd ~"
alias dl="cd ~/Downloads"
alias desk="cd ~/Desktop"
alias doc="cd ~/Documents"
alias docs="doc"
alias dotfiles="cd ~/.dotfiles"

alias proj="cd ~/Projects"
alias qmix="cd ~/Projects/QMix"
alias qdata="cd ~/Projects/SISData"
alias go="cd ~/Projects/GaussOpt"

export PATH="$HOME/.dotfiles/git/bin:$PATH"
export PATH="$HOME/Projects/QMix/bin:$PATH"
