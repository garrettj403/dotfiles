# PATHS
#
# Basic paths that are common to all my machines

alias home="cd ~"
alias dl="cd ~/Downloads"
alias desk="cd ~/Desktop"
alias doc="cd ~/Documents"

alias docs="cd ~/Documents"
alias dot="cd ~/.dotfiles"

alias gd="cd ~/GoogleDrive"
alias gdrive="cd ~/GoogleDrive"
alias proj="cd ~/GoogleDrive/Projects"
alias qmix="cd ~/GoogleDrive/Projects/QMix"
alias 230="cd ~/GoogleDrive/Projects/QMix/my-projects/230ghz-receiver"
alias freq="cd ~/GoogleDrive/Projects/QMix/my-projects/frequency-multiplier" 
alias go="cd ~/GoogleDrive/Projects/GaussOpt"
alias obs="cd ~/GoogleDrive/Projects/ObsWork"
alias notes="cd ~/GoogleDrive/Notes"

export PATH="$HOME/.dotfiles/git/bin:$PATH"
export PATH="$HOME/GoogleDrive/Projects/QMix/bin:$PATH"